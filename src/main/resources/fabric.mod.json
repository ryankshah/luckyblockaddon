{
	"schemaVersion": 1,
	"id": "luckyblockaddon",
	"version": "${version}",
	"name": "Lucky Block Addon",
	"description": "Hides lucky blocks as other blocks all around the world!",
	"authors": [
		"Me!"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/luckyblockaddon/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.liamkbryant.luckyblockaddon.LuckyBlockAddon"
		],
		"client": [
			"com.liamkbryant.luckyblockaddon.LuckyBlockAddonClient"
		],
		"fabric-datagen": [
			"com.liamkbryant.luckyblockaddon.LuckyBlockAddonDataGenerator"
		]
	},
	"accessWidener" : "luckyblockaddon.accesswidener",
	"mixins": [
		"luckyblockaddon.mixins.json",
		{
			"config": "luckyblockaddon.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.17.2",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*",
		"lucky": ">=1.20.1-13.0"
	},
	"suggests": {
		"another-mod": "*"
	}
}